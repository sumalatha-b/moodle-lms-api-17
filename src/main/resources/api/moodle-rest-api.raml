/core_user_get_users:
  post:
    is: [commonHeaders,errorResponse]
    displayName: User - Get Users
    description: Search for users matching criteria
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: token
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_user_get_users
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          criteria[0][key]:
            description: Search criteria key (e.g. username, email)
            type: string
            required: true
            example: firstname
          criteria[0][value]:
            description: Search criteria value
            type: string
            required: true
            example: Swayam
          # Add additional criteria as needed
    responses:
      200:
        body:
          application/json:
            type: coreUserGetUsersResponse
            example: !include /examples/user/getUsers-example.json
/core_user_get_users_by_field:
  post:
    is: [commonHeaders,errorResponse]
    displayName: User - Get Users by Id
    description: Retrieve users' information for a specified unique field
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: Token for authentication
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_user_get_users_by_field
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          field:
            description: Search field (id, idnumber, username, email)
            type: string
            required: true
            example: id
          values[0]:
            description: Value to match for the search field
            type: integer
            required: true
            example: 14
    responses:
      200:
        body:
          application/json:
            type: coreUserGetUsersByFieldResponse
            example: !include /examples/user/getUsersById-example.json
/core_user_create_users:
  post:
    is: [commonHeaders,errorResponse]
    displayName: User - Create users
    description: Create new users in Moodle
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: Token for authentication
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_user_create_users
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          users[0][createpassword]:
            description: True if password should be created and mailed to user
            type: integer
            example: 0
          users[0][username]:
            description: Username (must follow Moodle policy)
            type: string
            example: swayam2002
          users[0][auth]:
            description: Authentication plugin (default manual)
            type: string
            example: manual
          users[0][password]:
            description: User password (plain text)
            type: string
            example: Test@1234
          users[0][firstname]:
            type: string
            example: Swayam
          users[0][lastname]:
            type: string
            example: Prusty
          users[0][email]:
            type: string
            example: swayam@example.com
          users[0][city]:
            type: string
            example: Bhubaneswar
          users[0][country]:
            type: string
            example: IN
          users[0][idnumber]:
            type: string
            example: STU12345
          users[0][customfields][0][type]:
            type: string
            example: moodle_custom_field
          users[0][customfields][0][value]:
            type: string
            example: Custom Value
          users[0][preferences][0][type]:
            type: string
            example: maildigest
          users[0][preferences][0][value]:
            type: string
            example: preferencevalue1
    responses:
      200:
        body:
          application/json:
            type: coreUserCreateUsersResponse
            example: !include /examples/user/createUser-example.json
