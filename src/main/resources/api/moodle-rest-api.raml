    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: Token for authentication
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_user_get_users_by_field
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          field:
            description: Search field (id, idnumber, username, email)
            type: string
            required: true
            example: id
          values[0]:
