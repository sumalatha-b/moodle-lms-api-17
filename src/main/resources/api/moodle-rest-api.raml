/core_course_get_categories:
  post:
    is: [commonHeaders,errorResponse]
    displayName: Course Category - Get Categories
    description: Get list of course categories from Moodle with optional filtering criteria.
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: token
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_course_get_categories
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          criteria[0][key]:
            description: Key for filter (id, ids, name, parent, idnumber, visible, theme)
            type: string
            example: name
          criteria[0][value]:
            description: Value for filter key
            type: string
            example: Science
          addsubcategories:
            description: Return subcategories info (1 = yes, 0 = no)
            type: integer
            default: 1
    responses:
      200:
        body:
          application/json:
            type: coreCourseGetCategories
            example: !include /examples/courseCategory/getCategories-example.json

/core_course_create_categories:
  post:
    is: [commonHeaders,errorResponse]
    displayName: Course Category - Create Categories
    description: Create one or more new course categories
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: Moodle webservice token
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_course_create_categories
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          categories[0][name]:
            description: Category name
            type: string
            required: true
          categories[0][parent]:
            description: Parent category id (0 = root)
            type: integer
            default: 0
          categories[0][idnumber]:
            description: Category idnumber (optional)
            type: string
          categories[0][description]:
            description: Category description (optional)
            type: string
          categories[0][descriptionformat]:
            description: Description format (1=HTML, 0=MOODLE, 2=PLAIN, 4=MARKDOWN)
            type: integer
            default: 1
          categories[0][theme]:
            description: Theme, optional
            type: string
    responses:
      200:
        body:
          application/json:
            type: coreCourseCreateCategories
            example: !include /examples/courseCategory/createCategories-example.json

/core_course_update_categories:
  post:
    is: [commonHeaders,errorResponse]
    displayName: Course Category - Update Categories
    description: Update one or more existing course categories
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: Moodle webservice token
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_course_update_categories
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          categories[0][id]:
            description: Category id to update (required)
            type: integer
            required: true
            example: 5
          categories[0][name]:
            description: New category name (optional)
            type: string
            example: Updated Category Name
          categories[0][idnumber]:
            description: New category idnumber (optional)
            type: string
            example: CAT2025
          categories[0][parent]:
            description: New parent category id (optional)
            type: integer
            example: 0
          categories[0][description]:
            description: New description (optional)
            type: string
            example: Updated description here.
          categories[0][descriptionformat]:
            description: Description format (1=HTML, 0=MOODLE, 2=PLAIN, 4=MARKDOWN)
            type: integer
            default: 1
            example: 1
          categories[0][theme]:
            description: New theme name (optional)
            type: string
            example: default
    responses:
      200:
        body:
          application/json:
            type: commonResponse
            example: !include /examples/common/common-example.json
            
/core_course_delete_categories:
  post:
    is: [commonHeaders,errorResponse]
    displayName: Course Category - Delete Categories
    description: Delete one or more course categories
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: Moodle web service token
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_course_delete_categories
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          categories[0][id]:
            description: Category id to delete (required)
            type: integer
            required: true
            example: 123
          categories[0][newparent]:
            description: Category id to move contents to (optional, cannot be root)
            type: integer
            example: 5
          categories[0][recursive]:
            description: Delete recursively flag (1 = delete all contents recursively, 0 = move contents to newparent)
            type: integer
            default: 0
            example: 0
    responses:
      200:
        body:
          application/json:
            type: commonResponse
            example: !include /examples/common/common-example.json






/enrol_manual_enrol_users:
  post:
    is: [commonHeaders,errorResponse]
    displayName: Enrollment - Enroll Users
    description: Enrol users into a course in Moodle
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: Token
            type: string
            required: true
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: enrol_manual_enrol_users
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          enrolments[0][roleid]:
            description: Role to assign 
            type: integer
            required: true
            example: 5
          enrolments[0][userid]:
            description: User ID to enrol
            type: integer
            required: true
            example: 15
          enrolments[0][courseid]:
            description: Course ID to enrol user in
            type: integer
            required: true
            example: 8
          enrolments[0][timestart]?:
            description: Optional enrolment start timestamp
            type: integer
            example: 1689264000
          enrolments[0][timeend]?:
            description: Optional enrolment end timestamp
            type: integer
            example: 1691865600
          enrolments[0][suspend]?:
            description: Suspend status 
            type: integer
            example: 0
    responses:
