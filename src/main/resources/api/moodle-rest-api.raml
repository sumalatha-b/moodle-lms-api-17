/core_course_get_courses:
  post:
    is: [commonHeaders,errorResponse]
    displayName: Course - Get Courses
    description: Get list of courses from Moodle
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: token
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_course_get_courses
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
    responses:
      200:
        body:
          application/json:
            type: coreCourseGetCourses
            example: !include /examples/courses/coreCourseGetCourse-example.json

/core_course_create_courses:
  post:
    is: [commonHeaders,errorResponse]
    displayName: Course - Create Courses
    description: Create Courses
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: token
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_course_create_courses
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          courses[0][fullname]:
            type: string
            required: true
            example: Sample Full Course Name
          courses[0][shortname]:
            type: string
            required: true
          courses[0][categoryid]:
            type: integer
            required: true
          courses[0][idnumber]:
            type: string
            required: true
          courses[0][summary]:
            type: string
            required: false
          courses[0][summaryformat]?: integer
          courses[0][format]?: string
          courses[0][showgrades]?: integer
          courses[0][newsitems]?: integer
          courses[0][startdate]?: integer
          courses[0][enddate]?: integer
          courses[0][numsections]?: integer
          courses[0][maxbytes]?: integer
          courses[0][showreports]?: integer
          courses[0][visible]?: integer
          courses[0][hiddensections]?: integer
          courses[0][groupmode]?: integer
          courses[0][groupmodeforce]?: integer
          courses[0][defaultgroupingid]?: integer
          courses[0][enablecompletion]?: integer
          courses[0][completionnotify]?: integer
          courses[0][lang]?: string
          courses[0][forcetheme]?: string
          courses[0][courseformatoptions][0][name]?: string
          courses[0][courseformatoptions][0][value]?: integer
          courses[0][customfields][0][shortname]?: string
          courses[0][customfields][0][value]?: string
    responses:
      200:
        body:
          application/json:
            type: createCourse
            example: !include /examples/courses/createCourse-example.json

