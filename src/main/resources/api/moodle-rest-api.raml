/core_course_get_courses:
  post:
    is: [commonHeaders,errorResponse]
    displayName: Course - Get Courses
    description: Get list of courses from Moodle
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: token
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_course_get_courses
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
    responses:
      200:
        body:
          application/json:
            type: coreCourseGetCourses
            example: !include /examples/courses/coreCourseGetCourse-example.json

