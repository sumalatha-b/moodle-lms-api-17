        properties:
          wstoken:
            type: string
            required: true
          wsfunction:
            type: string
            enum: [ core_cohort_create_cohorts ]
            required: true
          moodlewsrestformat:
            type: string
            enum: [ json ]
            required: true
          cohorts[0][categorytype][type]:
            type: string
            required: true
            example: system
          cohorts[0][categorytype][value]:
            type: string
            required: true
            example: "0"
          cohorts[0][name]:
            type: string
            required: true
            example: IT Cohort 123
          cohorts[0][idnumber]:
            type: string
            required: true
            example: ITC123
          cohorts[0][description]?:
            type: string
            example: This is a test cohort
          cohorts[0][descriptionformat]?:
            type: string   # must be string because x-www-form-urlencoded encodes numbers as strings
            example: "1"
          cohorts[0][visible]?:
            type: string   # keep string here, not integer
            example: "1"
          cohorts[0][theme]?:
            type: string
            example: boost
