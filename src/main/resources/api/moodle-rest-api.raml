  getCourseByField: !include /datatypes/courses/getCoursesByField-dataType.raml
  updateDeleteCourse: !include /datatypes/courses/updateDeleteCourse-dataType.raml
  
  getAssignments: !include /datatypes/assignments/getAssignments-dataType.raml
  getAssignmentSubmission: !include /datatypes/assignments/getAssignmentSubmission-dataType.raml
  getAssignmentGrades: !include /datatypes/assignments/getAssignmentGrades-dataType.raml
  coreUserGetUsersResponse: !include /datatypes/user/getUsers-dataType.raml
  coreUserGetUsersByFieldResponse: !include /datatypes/user/getUsersById-dataType.raml
  coreUserCreateUsersResponse: !include /datatypes/user/createUser-dataType.raml
  coreUserUpdateUsersResponse: !include /datatypes/user/updateUser-dataType.raml
  
  coreCourseGetCategories: !include /datatypes/courseCategory/getCategories-dataType.raml
  coreCourseCreateCategories: !include /datatypes/courseCategory/createCategories-dataType.raml
  getCourseGrades: !include /datatypes/grades/getCourseGrades-dataType.raml
  enrolManualEnrolUsers: !include /datatypes/enrollment/enrolManualEnrolUsers-dataType.raml
  enrolManualUnenrolUsers: !include /datatypes/enrollment/enrolManualUnenrolUsers-dataType.raml
