          users[0][id]:
            description: User ID to be updated
            type: integer
            required: true
            example: 21
          users[0][firstname]:
            type: string
            example: Swayam Updated
          users[0][lastname]:
            type: string
            example: Prusty
          users[0][email]:
            type: string
            example: swayam.updated@example.com
          users[0][suspended]:
            type: integer
            example: 0
          users[0][customfields][0][type]:
            type: string
            example: moodle_custom_field
          users[0][customfields][0][value]:
            type: string
