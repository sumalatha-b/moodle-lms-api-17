/core_user_create_users:
  post:
    is: [commonHeaders,errorResponse]
    displayName: User - Create users
    description: Create new users in Moodle
    body:
      application/x-www-form-urlencoded:
        properties:
          wstoken:
            description: Token for authentication
            type: string
            required: true
            example: db6ccaf6e69123c284e4a5710be8b875
          wsfunction:
            description: Moodle function name
            type: string
            required: true
            example: core_user_create_users
          moodlewsrestformat:
            description: Response format
            type: string
            required: true
            example: json
          users[0][createpassword]:
            description: True if password should be created and mailed to user
            type: integer
            example: 0
          users[0][username]:
            description: Username (must follow Moodle policy)
            type: string
            example: swayam2002
          users[0][auth]:
            description: Authentication plugin (default manual)
            type: string
            example: manual
          users[0][password]:
            description: User password (plain text)
            type: string
            example: Test@1234
          users[0][firstname]:
            type: string
            example: Swayam
          users[0][lastname]:
            type: string
            example: Prusty
          users[0][email]:
            type: string
            example: swayam@example.com
          users[0][city]:
            type: string
            example: Bhubaneswar
          users[0][country]:
            type: string
